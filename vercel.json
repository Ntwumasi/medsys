{
  "framework": null,
  "buildCommand": "cd client && npm install && npm run build && cd .. && mkdir -p dist && cp -r client/dist/* dist/",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.2.23"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
